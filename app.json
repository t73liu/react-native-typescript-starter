{
  "expo": {
    "name": "TypeScript To-Do App",
    "description": "TypeScript To-Do app using Redux",
    "slug": "typescript-starter",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android"],
    "version": "2.0.0",
    "githubUrl": "https://github.com/t73liu/react-native-typescript-starter",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#CDC449"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "io.github.t73liu.boilerplate",
      "supportsTablet": true
    },
    "android": {
      "package": "io.github.t73liu.boilerplate"
    }
  }
}
